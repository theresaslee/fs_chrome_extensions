{
  //Required
  "name": "Fullstack Backgrounds",
  "version": "1.0",
  "manifest_version": 2,

  //Recommended
  "description": "Because Fullstack should be everywhere!",
  "icons": {
    "128": "fullstackicon.png"
  },

  //Type of action: Browser,Page, or none
  "browser_action": {
      "default_title": "Always have the Fullstack team with you <3",
      "default_popup": "popup.html"
  },

  //intent of app. what you will access from the user
  "permissions": [
    "http://*/*", "https://*/*"
  ]
}
